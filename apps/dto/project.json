{
  "name": "dto",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dto/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/dto",
        "tsConfig": "apps/dto/tsconfig.lib.json",
        "packageJson": "apps/dto/package.json",
        "main": "apps/dto/src/index.ts",
        "assets": ["apps/dto/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/dto/**/*.ts", "apps/dto/package.json"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/dto/jest.config.ts"
      }
    }
  },
  "tags": []
}
